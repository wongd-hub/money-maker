{
  "name": "Auto Timesheet Extension",
  "description": "Captures a screenshot of your timesheet and sends via email",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "identity",
    "tabs",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "Capture Timesheet"
  },
  "background": {
    "service_worker": "service_worker.js"
  }
}
